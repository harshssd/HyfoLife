{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev",
      "android": { 
        "gradleCommand": ":app:assembleDebug" 
      },
      "ios": { 
        "resourceClass": "m-medium" 
      }
    },
    "beta": {
      "channel": "beta",
      "android": { 
        "buildType": "apk" 
      },
      "ios": { 
        "simulator": false 
      }
    },
    "production": {
      "channel": "production",
      "ios": { 
        "simulator": false 
      },
      "android": { 
        "buildType": "app-bundle" 
      }
    }
  },
  "submit": {
    "beta": {
      "ios": { 
        "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID" 
      },
      "android": { 
        "track": "internal" 
      }
    },
    "production": {
      "ios": { 
        "ascAppId": "YOUR_APP_STORE_CONNECT_APP_ID" 
      },
      "android": { 
        "track": "production" 
      }
    }
  }
}
